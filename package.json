{
  "name": "ImageArchive",
  "main": "./dist/src/main.js",
  "type": "module",
  "scripts": {
    "babel": "npx babel src --out-dir tmp",
    "build": "npx webpack && pkg ."
  },
  "dependencies": {
    "sqlite3": "^5.1.7",
    "windows-shortcuts": "^0.1.6"
  },
  "bin": {
    "ImageArchive": "./dist/src/main.js"
  },
  "pkg": {
    "scripts": "./dist/src/*.js",
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist",
    "options": [
      "experimental-modules"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/preset-env": "^7.24.5",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "file-loader": "^6.2.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
